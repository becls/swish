#!/bin/sh -e
launch="scheme --eedisable -q replp.ss run-mats.ss"

find . -name *.mo -delete
find . -name *.html -and -not -iwholename */web-mats/* -delete
rm -f ../data/server.profile
rm -f ../data/TestLog.db3
make -s -C swish

$launch <<EOF
(run-suite "$1" ".")
EOF

# finish up
$launch <<EOF
(html-report "." "suite-report.html")
(console-summary ".")
EOF

./dump-profile $bits
